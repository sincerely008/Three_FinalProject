<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="payment">

	<insert id="insertpay">
		INSERT INTO PAYMENT VALUES(PAYMENTSEQ.NEXTVAL, #{payment_groupno}, #{member_id}, #{product_no}, #{payment_name}, #{payment_price}, SYSDATE, #{payment_addr})
	</insert>
	
</mapper>
